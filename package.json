    "file-loader": "^2.0.0",
    "fs": "^0.0.1-security",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.3",
    "pug-html-loader": "^1.1.5",
    "pug-loader": "^2.4.0",
    "resolve-url-loader": "^3.0.0",
