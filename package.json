  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:watch": "webpack --mode development --watch --colors --progress",
    "prod:watch": "webpack --mode production --watch --colors --progress --config webpack.config.prod.js",
    "prod": "webpack --mode production --colors --progress --config webpack.config.prod.js --json > dist/stats.json",
    "dev": "webpack --mode development",
    "start": "webpack-dev-server --colors --progress --open",
    "build": "webpack --mode production",
    "push-pages": "git subtree push --prefix dist origin gh-pages",
    "push": "git push origin master && git subtree push --prefix dist origin gh-pages"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
